<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default sync-j="4" revision="jethro"/>

  <remote name="digi"  fetch="https://github.com/digi-embedded"/>
  <remote name="fsl"   fetch="https://github.com/Freescale"/>
  <remote name="qt5"   fetch="https://github.com/meta-qt5"/>
  <remote name="swu"   fetch="https://github.com/sbabic"/>
  <remote name="oe"    fetch="https://github.com/openembedded"/>

  <project name="meta-digi" path="sources/meta-digi" remote="digi" revision="jethro">
    <copyfile src="sdk/mkproject.sh" dest="mkproject.sh"/>
  </project>
  <project name="meta-fsl-arm"      path="sources/meta-fsl-arm"      remote="fsl"   revision="jethro"/>
  <project name="meta-fsl-demos"    path="sources/meta-fsl-demos"    remote="fsl"   revision="jethro"/>
  <project name="meta-openembedded" path="sources/meta-openembedded" remote="oe"    revision="jethro"/>
  <project name="meta-qt5"          path="sources/meta-qt5"          remote="qt5"   revision="jethro"/>
  <project name="meta-swupdate"     path="sources/meta-swupdate"     remote="swu"   revision="jethro"/>
  <project name="poky"              path="sources/poky"              remote="digi"  revision="jethro"/>
</manifest>
